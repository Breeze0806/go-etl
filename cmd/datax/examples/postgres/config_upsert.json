{
    "core" : {
        "container": {
            "job":{
                "id": 1,
                "sleepInterval":100
            }
        }
    },
    "job":{
        "content":[
            {
                "reader":{
                    "name": "postgresreader",
                    "parameter": {
                        "username": "user1",
                        "password": "password1",
                        "column": ["*"],
                        "connection":  {
                                "url": "postgres://127.0.0.1:5432/db1?sslmode=disable&connect_timeout=2",
                                "table": {
                                    "schema":"source",
                                    "name":"type_table"
                                }
                            },
                        "where": ""
                    }
                },
                "writer":{
                    "name": "postgreswriter",
                    "parameter": {
                        "username": "user2",
                        "password": "password2",
                        "writeMode": "upsert",
                        "column": ["*"],
                        "preSql": [],
                        "connection":  {
                                "url": "postgres://127.0.0.1:5433/db2?sslmode=disable&connect_timeout=2",
                                "table": {
                                    "schema":"destination",
                                    "name":"type_table"
                                }
                         },
                        "upsertSql":"on conflict (t_bigserial) do update set t_smallserial=excluded.t_smallserial",
                        "batchTimeout": "1s",
                        "batchSize":1000
                    }
                },
               "transformer":[]
            }
        ]
    }
}